   "source": [
    "a = int(input(\"enter the  value  to find the value of sum of digits : \"))\n",
    "\n",
    "s = 0\n",
    "n = a \n",
    "while n>0:\n",
    "    r = n%10\n",
    "    s = s+r\n",
    "    n = n//10\n",
    "    \n",
